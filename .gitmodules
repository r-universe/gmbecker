	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "GRANCore"]
	path = GRANCore
	url = https://github.com/gmbecker/grancore
	shallow = true
	registered = true
